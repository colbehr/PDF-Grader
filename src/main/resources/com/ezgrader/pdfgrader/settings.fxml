<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<AnchorPane prefHeight="256.0" prefWidth="150.0" stylesheets="@shared.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ezgrader.pdfgrader.SettingsController">
   <children>
       <Button mnemonicParsing="false" prefHeight="77.0" prefWidth="150.0" text="Shortcuts" translateY="25.0" onAction="#openShortcuts"/>
       <Button mnemonicParsing="false" prefHeight="77.0" prefWidth="150.0" text="Set Folder" translateY="105.0" onAction="#openFolder"/>
       <Button mnemonicParsing="false" prefHeight="77.0" prefWidth="150.0" text="Set File" translateY="185.0" onAction="#selectFile"/>
      <MenuBar prefHeight="25.0" prefWidth="150.0">

      </MenuBar>

       <Button mnemonicParsing="false" translateX="-25.0" onAction="#closeSettings">
           <graphic>
               <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                   <image>
                       <Image url="@img/redX.png" />
                   </image></ImageView>
           </graphic>
           <padding>
               <Insets left="25.0" />
           </padding>
           <cursor>
               <Cursor fx:constant="HAND" />
           </cursor>
           <VBox.margin>
               <Insets bottom="5.0" />
           </VBox.margin>
       </Button>
<!--      <Button mnemonicParsing="false" text="Button" translateX="50.0"-->
<!--      />-->
   </children>
</AnchorPane>
